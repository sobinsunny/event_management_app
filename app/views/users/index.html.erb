<% if @users.present?%>
  <table class="table table-striped" >
    <thead>
      <tr>
        <th>Name</th>
        <th>Gender</th>
      </tr>
    </thead>
    <tbody>
      <%@users.each do |user| %>
      <tr>
        <td><%=user.name%></td>
        <td><%=user.gender%></td>
        <td><%=user.email%></td>
      </tr>
      <% end %>
    </tbody>
  </table>
  <%else%>
    No Members.
  <%end%>
</div>